name:           hs-boot
version:        0.1
cabal-version:  >= 1.10
build-type:     Simple

executable hs-boot
  main-is: Main.hs
  hs-source-dirs:
      src
  ghc-options: -Wall -fno-warn-name-shadowing
  build-depends:
      base
    , base-compat
    , logging-facade
    , directory
  default-language: Haskell2010

test-suite spec
  type: exitcode-stdio-1.0
  ghc-options: -Wall -fno-warn-name-shadowing
