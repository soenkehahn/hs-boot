      test
    , src
  main-is: Spec.hs
  build-depends:
      base
    , base-compat
    , logging-facade
    , directory
    , hspec
    , setenv
    , filepath
    , silently
    , process
    , mockery
  other-modules:
      MainSpec
      Main
  default-language: Haskell2010
